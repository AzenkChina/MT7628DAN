config ser2net global
	option enabled 1

config controlport
	option enabled 0
	option host localhost
	option port 2000

config default
	option speed 9600
	option databits 8
	option parity 'none'
	option stopbits 1
	option rtscts false
	option local false
	option remctl false

config proxy
	option enabled 1
	option port 5000
	option protocol raw
	option timeout 0
	option device '/dev/ttyS1'

config proxy
	option enabled 1
	option port 5001
	option protocol raw
	option timeout 0
	option device '/dev/ttyS2'
